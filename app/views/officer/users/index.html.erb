<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<div id="users">
  <% @users.each do |user| %>
    <%= render 'officer/users/user', user: user %>
    <p>
      <%= link_to "Show this user", officer_user_path(user) %>
    </p>
  <% end %>
</div>

<%= link_to 'Back to Officer Dashboard', officer_root_path %>
