<div id="<%= dom_id user %>">
  <p>
    <strong>Access level:</strong>
    <%= user.access_level %>
  </p>

  <p>
    <strong>First Name:</strong>
    <%= user.first_name %>
  </p>
  <p>
    <strong>Last Name:</strong>
    <%= user.last_name %>
  </p>

  <p>
    <strong>Year:</strong>
    <%= user.year %>
  </p>

  <p>
    <strong>Phone number:</strong>
    <%= user.phone_number %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

</div>
